<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.RuoLiang.MyServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/com.RuoLiang.MyServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <display-name>AddGoodServlet</display-name>
        <servlet-name>AddGoodServlet</servlet-name>
        <servlet-class>com.RuoLiang.AddGoodServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddGoodServlet</servlet-name>
        <url-pattern>/com.RuoLiang.AddGoodServlet</url-pattern>
    </servlet-mapping>
</web-app>